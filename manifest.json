{
    "manifest_version": 2,
    "version": "0.1.2",
  
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
  
    "icons": {
        "48": "assets/icon.svg"
    },

    "permissions": [
        "<all_urls>",
        "downloads", "storage", "tabs", "notifications", "webRequest" ,"webRequestBlocking", "cookies", "contextMenus"
    ],
  
    "default_locale": "en",

    "browser_action": {
        "default_icon": "assets/icon.svg",
        "default_title": "__MSG_extensionName__",
        "default_popup": "popup/popup.html"
    },   

    "background": {
        "page": "background/background.html"
    },

    "content_scripts": [
    ],

    "protocol_handlers": [
        {
          "protocol": "magnet",
          "name": "Dsender",
          "uriTemplate": "/pages/protocol/magnet.html?url=%s"
        }
    ],

    "developer": {
       "name": "catsout",
       "url": "https://github.com/catsout/Dsender"
    }
}
