{
    "manifest_version": 2,
    "version": "1.0",
  
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
  
    "icons": {
        "48": "icons/no.png"
    },

    "permissions": [
        "<all_urls>",
        "downloads", "storage", "tabs"
    ],
  
    "default_locale": "en",

    "browser_action": {
        "default_icon": "icons/no.png",
        "default_title": "__MSG_extensionName__",
        "default_popup": "popup/popup.html"
    },   

    "background": {
        "page": "background/background.html"
    },

    "content_scripts": [
    ],

    "protocol_handlers": [
    {
      "protocol": "magnet",
      "name": "Dsender",
      "uriTemplate": "/pages/protocol/magnet.html?url=%s"
    }
  ]
}
